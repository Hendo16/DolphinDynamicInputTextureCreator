<Application x:Class="DolphinDynamicInputTextureCreator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DolphinDynamicInputTextureCreator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="resources\icongeometry.xaml"/>
                <ResourceDictionary Source="resources\icons.xaml"/>
                <ResourceDictionary Source="resources\buttonstyles.xaml"/>
                <ResourceDictionary Source="resources\InputRegionRectStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
